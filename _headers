# Cache static assets for 1 year (immutable)
/styles.min.css
  Cache-Control: public, max-age=31536000, immutable

/script.min.js
  Cache-Control: public, max-age=31536000, immutable

/profile.webp
  Cache-Control: public, max-age=31536000, immutable

/profile.png
  Cache-Control: public, max-age=31536000, immutable

/featured-projects/*
  Cache-Control: public, max-age=31536000, immutable

# HTML and other - revalidate
/*
  Cache-Control: public, max-age=0, must-revalidate
